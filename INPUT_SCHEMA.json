    "spreadsheetId": {
      "title": "Spreadsheet ID",
      "type": "string",
      "description": "Spreadsheet should contains on the first sheet 3 columns in the respective order: City, Country, and Category.",
      "prefill": "",
      "editor": "textfield"
    },
    "publicSpreadsheet": {
      "title": "Public Spreadsheet",
      "type": "boolean",
      "description": "The spreadsheet is public.",
      "default": false
    },
    "proxyConfig": {
      "title": "Proxy configuration",
      "type": "object",
      "description": "Use Apify Proxy, you need to have some proxy group to get results on Google.",
